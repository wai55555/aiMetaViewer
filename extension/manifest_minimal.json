{
    "manifest_version": 3,
    "name": "AI Meta Viewer Test",
    "version": "1.0.0",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "service_worker": "background_test.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_test.js"
            ]
        }
    ]
}